<?xml version='1.0' encoding='utf-8'?>
<widget id="com.two.mobility.iTWO40BusinessPartner" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Business Partner</name>
    <description>
        iTWO 4.0 Business Partner.
    </description>
    <author email="Eddy.Shan@ribitwo.com" href="http://www.rib-software.com/">
        RIB Software SE
    </author>
    <content src="index.html" />

    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar"/>
    </feature>


    <preference name="Fullscreen" value="false" />

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />

        <icon src="www/platformResources/Android/icon/drawable-ldpi-icon.png" density="ldpi" />
        <icon src="www/platformResources/Android/icon/drawable-mdpi-icon.png" density="mdpi" />
        <icon src="www/platformResources/Android/icon/drawable-xhdpi-icon.png" density="xhdpi" />

        <splash src="www/platformResources/Android/splash/drawable-land-hdpi-screen.png" density="land-hdpi"/>
        <splash src="www/platformResources/Android/splash/drawable-land-ldpi-screen.png" density="land-ldpi"/>
        <splash src="www/platformResources/Android/splash/drawable-land-mdpi-screen.png" density="land-mdpi"/>
        <splash src="www/platformResources/Android/splash/drawable-land-xhdpi-screen.png" density="land-xhdpi"/>

        <splash src="www/platformResources/Android/splash/drawable-port-hdpi-screen.png" density="port-hdpi"/>
        <splash src="www/platformResources/Android/splash/drawable-port-ldpi-screen.png" density="port-ldpi"/>
        <splash src="www/platformResources/Android/splash/drawable-port-mdpi-screen.png" density="port-mdpi"/>
        <splash src="www/platformResources/Android/splash/drawable-port-xhdpi-screen.png" density="port-xhdpi"/>

    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />

        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone Spotlight-ios5,6 settings- iOS 5-10 29pt/1x.png" width="29" height="29" />
        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone Spotlight-ios5,6 settings- iOS 5-10 29pt/1x.png" width="58" height="58" />
        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone Spotlight-ios5,6 settings- iOS 5-10 29pt/1x.png" width="87" height="87" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad spotlight ios7-10 40pt/1x.png" width="40" height="40" />
        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone Spotlight-ios7-10 40pt/2x.png" width="80" height="80" />
        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone Spotlight-ios7-10 40pt/3x .png" width="120" height="120" />
        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone App ios7-10 60pt/2x.png" width="120" height="120" />
        <icon src="www/platformResources/iOS/AppIcon/iPhone/iphone App ios7-10 60pt/3x.png" width="180" height="180" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad Setting ios 5-10 29pt/1x.png" width="29" height="29" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad Setting ios 5-10 29pt/2x.png" width="57" height="57" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad App ios5,6 72pt/1x.png" width="72" height="72" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad App ios5,6 72pt/2x.png" width="144" height="144" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad App ios 7-10 76pt/1x.png" width="76" height="76" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad App ios 7-10 76pt/2x.png" width="152" height="152" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad spotlight ios 5,6 50pt/1x.png" width="50" height="50" />
        <icon src="www/platformResources/iOS/AppIcon/iPad/iPad spotlight ios 5,6 50pt/2x.png" width="100" height="100" />

        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone protrait ios5,6/1x.png" width="320" height="480" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone protrait ios5,6/2x.png" width="640" height="960" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone protrait ios5,6/Retina4..png" width="640" height="1136" />
        <splash src="www/platformResources/iOS/LaunchImage/iPad/iPad porerait ios5,6/1x.png" width="768" height="1024" />
        <splash src="www/platformResources/iOS/LaunchImage/iPad/iPad porerait ios5,6/2x.png" width="1536" height="2048" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone Portrait ios 8,9/Retina HD 4.7.png" width="1242" height="2208" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone Portrait ios 8,9/Retina HD 5.5.png" width="750" height="1334" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone Landscape ios 8,9/Retina HD 5.5.png" width="2208" height="1242" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone Portrait ios 8,9/Retina HD 4.4.png" width="640" height="960" />
        <splash src="www/platformResources/iOS/LaunchImage/iPhone/iPhone Portrait ios 8,9/Retina HD 5.5.png" width="640" height="1136" />
        <splash src="www/platformResources/iOS/LaunchImage/iPad/ipad portrait ios7-9/1x.png" width="768" height="1024" />
        <splash src="www/platformResources/iOS/LaunchImage/iPad/ipad portrait ios7-9/2x.png" width="1536" height="2048" />
        <splash src="www/platformResources/iOS/LaunchImage/iPad/ipad landscape ios7-9/1x.png" width="1024" height="768" />
        <splash src="www/platformResources/iOS/LaunchImage/iPad/ipad landscape ios7-9/2x.png" width="2048" height="1536" />

        <config-file target="*-Info.plist" parent="CFBundleURLTypes" mode="add">
            <array>
                <dict>
                    <key>ITSAppUsesNonExemptEncryption</key>
                    <false/>
                </dict>
            </array>
        </config-file>
    </platform>
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.1" />
    <plugin name="cordova-plugin-console" spec="1.0.5" />
    <plugin name="cordova-plugin-statusbar" spec="2.2.2" />
    <plugin name="cordova-plugin-device" spec="1.1.4" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.1" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-contacts" spec="~2.3.1">
        <variable name="CONTACTS_USAGE_DESCRIPTION" value=" " />
    </plugin>
</widget>
