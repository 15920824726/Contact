#contacts-list .contact-item-img img {
    max-width: 50px;
    max-height: 60px;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    margin-right: 20px;
}

#contacts-list .list ion-item {
    border: none;
    border-bottom: 1px #d3d3d3 solid;
    margin-bottom: 1px;
}

#contacts-list ion-list .list {
    margin-top: -10px;
}

#contacts-list .list-inset {
    margin: 0;
    padding: 10px 10px;
    background-color: #e7eef4;
}

#contacts-list ion-header-bar {
    background-color: #0167b1;
}

#contacts-list ion-header-bar button{
    /*position: absolute;*/
    /*right: 16px;*/
    /*top:4px;*/
}

#contacts-list ion-list .contact-item-detail i.contact-icon-company{
    opacity: 0.7;
}

#contacts-list ion-content div.list{
    margin-top: 55px;
}

#contacts-list ion-content div.scroll.padding{
    height: 100%;
}

#contacts-list .list-inset a{
    margin-right: 20px;
    padding: 10px;
}


#contacts-list ion-item a{
    padding: 10px;
}

