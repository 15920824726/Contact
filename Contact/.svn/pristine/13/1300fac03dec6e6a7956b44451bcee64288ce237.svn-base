angular.module('App.common').constant('phoneHelper', {
    call: function (tel) {
        window.location.href = 'tel:' + tel;
    },
    email: function (mail) {
        window.location.href = 'mailto:' + mail;
    },
    message: function (tel) {
        window.location.href = 'sms:' + tel;
    }
});
